{
  "name": "@chili-and-cilantro/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "new:secret": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\"",
    "build:all": "npx nx build chili-and-cilantro-lib && npx nx build chili-and-cilantro-react && npx nx build chili-and-cilantro-api",
    "build:all:dev": "npx nx build chili-and-cilantro-lib --configuration development && npx nx build chili-and-cilantro-react --configuration development && npx nx build chili-and-cilantro-api --configuration development",
    "build:node": "npx nx build chili-and-cilantro-api",
    "build:node:dev": "npx nx build chili-and-cilantro-api --configuration development",
    "build:react": "npx nx build chili-and-cilantro-react",
    "build:react:dev": "npx nx build chili-and-cilantro-react --configuration development",
    "build:lib": "npx nx build chili-and-cilantro-lib",
    "build:lib:dev": "npx nx build chili-and-cilantro-lib --configuration development",
    "build:react:serve": "npx nx serve chili-and-cilantro-react",
    "build": "yarn build:all",
    "build:dev": "yarn build:all:dev",
    "build-serve:dev": "npx nx build chili-and-cilantro-lib --configuration development && npx nx build chili-and-cilantro-react --configuration development && npx nx serve chili-and-cilantro-api --configuration development",
    "serve:dev": "npx nx serve chili-and-cilantro-api --configuration development",
    "npm-globals": "npm install -g yarn @nrwl/cli npx nx jest",
    "clean:all": "rm -rf node_modules dist/ ~/.cache && yarn cache clean",
    "migrate:latest": "npx nx migrate latest",
    "migrate:run-migrations": "npx nx migrate --run-migrations",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
    "prettier:check": "prettier --check '**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx}'"
  },
  "private": true,
